{
  "name": "nestapi",
  "version": "1.0.0",
  "description": "API backend service with REST and GrapQL based on Nest.js, TypeORM, Apollo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:auto": "npm run build && npm run typeorm migration:generate -- -d ./src/config/typeorm.config.ts ./src/typeorm/migrations/auto",
    "migration:create": "npm run build && npm run typeorm migration:create -- -n",
    "migration:fake": "npm run migration:run -- --fake",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run build && npm run typeorm migration:run -- -d ./src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@casl/ability": "^6.5.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.3",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.1",
    "os": "^0.1.2",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "puppeteer": "^24.3.0",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "session-file-store": "^1.5.0",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.16",
    "typeorm-model-generator": "^0.4.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/babel-generator": "^6.25.8",
    "@types/babel__core": "^7.20.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jest": "29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "18.15.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.38",
    "@types/passport-oauth2": "^1.4.15",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
